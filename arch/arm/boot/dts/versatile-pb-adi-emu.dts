#include "versatile-pb.dts"
#include <dt-bindings/gpio/gpio.h>
/ {
	spi_adi_emu@0 {
		compatible = "adi,spi-adi-emu";
		#address-cells = <1>;
		#size-cells = <0>;

		iio_adi_emu@0 {
			compatible = "iio-adi-emu";
			reg = <0x0>;
			spi-max-frequency = <1000000>;
		};
	};
	
	 gpio_adrf5740: adrf5740 {
	      compatible = "adi,adrf5740";
	      ctrl-gpios = <&gpio0 0 GPIO_ACTIVE_HIGH>,
		<&gpio1 1 GPIO_ACTIVE_HIGH>,
		<&gpio2 2 GPIO_ACTIVE_HIGH>,
		<&gpio3 3 GPIO_ACTIVE_HIGH>;
    };
	
};
